{
  "segments": {
    "left": [{
      "function": "powerline.segments.common.external_ip"
    }],
    "right": [{
      "function": "powerline.segments.common.email_imap_alert",
      "name": "email",
      "args": {
        "username": "",
        "password": ""
      }
    }, {
      "function": "powerline.segments.common.hostname",
      "args": {
        "only_if_ssh": true
      }
    }, {
      "function": "powerline.segments.common.branch",
      "args": {
        "status_colors": true
      }
    }, {
      "function": "powerline.segments.common.internal_ip",
      "args": {
        "interface": "detect",
        "ipv": 4
      }
    }, {
      "function": "powerline.segments.common.virtualenv"
    }, {
      "function": "powerline.segments.common.weather",
      "args": {
        "unit": "F",
        "location_query": "Ocoee"
      }
    }, {
      "function": "powerline.segments.common.date",
      "args": {
        "format": "%c",
        "istime": false
      }
    }]
  }
}
